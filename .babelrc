{
  "presets": ["@babel/react","@babel/env"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "lodash",
    ["module-resolver",{
      "root": ["./src"],
      "alias": {
         "images": "./src/images",
         "actions": "./src/actions",
          "components": "./src/components",
          "containers": "./src/containers",
          "modules": "./src/modules"
      }

    }]
  ]
}